%SIGNAL
PIN   4 =  A1 
PIN   5 =  A2 
PIN   6 =  A3 
PIN   7 =  A4 
PIN   1 =  CLOCK 
PIN  16 = !CS1 
PIN  17 = !CS3 
PIN  27 =  Ca 
PIN  26 =  Cb 
PIN  21 =  DTACK 
PIN  25 =  DTACK_ENABLE 
PIN   9 =  FC0 
PIN  10 =  FC1 
PIN  11 =  FC2 
PIN   8 =  INTRQ 
PIN  18 = !IRQ 
PIN   2 = !LDS 
PIN  22 = !LSEL 
PIN  14 = !RD 
PIN  13 =  R_nW 
PIN  23 = !SELECT 
PIN   3 = !UDS 
PIN  19 = !USEL 
PIN  20 = !VPA 
PIN  15 = !WR 
