%FIELD
FIELD STATE = Cb,Ca,DTACK,DTACK_ENABLE
%END

%EQUATION
CS1 =>
    !A4 & !SELECT

CS3 =>
    A4 & !SELECT

Ca.d  =>
    Ca & Cb & !DTACK
